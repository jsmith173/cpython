import pymatrix
 
R1 = 1000
R2 = 2000
U  = 5

#Equations 
# Fi1/R1+(Fi1-U)/R2+(Fi1-Fi2)/R2 = 0
# Fi2/R1+(Fi2-U)/R1+(Fi2-Fi1)/R2 = 0
 
# Coefficients 
#Fi1: 1/R1+2/R2 
#Fi2: -1/R2
#b0: U/R2

#Fi1: -1/R2
#Fi2: 2/R1+1/R2
#b1: U/R1

# Ax = b
# x = inv(A)*b
list = [[1/R1+2/R2, -1/R2], [-1/R2, 2/R1+1/R2]]
list_b = [[U/R2], [U/R1]]
A = pymatrix.Matrix.from_list(list)
b = pymatrix.Matrix.from_list(list_b)
print(A)
print("")

print(b)
print("")

invA = A.inv()
print(invA)
print("")

x = invA*b
